		"devdoc": {
			"events": {
				"ConfirmTransaction(address,uint256)": {
					"details": "记录确认交易日志",
					"params": {
						"owner": "合约所有者提出的确认票",
						"txIndex": "交易序号"
					}
				},
				"Deposit(address,uint256,uint256)": {
					"details": "接收外部账户转账记录",
					"params": {
						"amount": "交易金额",
						"balance": "转账后合约账户的金额",
						"sender": "转账的外部账户地址"
					}
				},
				"ExecuteTransaction(address,uint256)": {
					"details": "记录执行交易日志",
					"params": {
						"owner": "发起交易执行的合约所有者地址",
						"txIndex": "合约的索引"
					}
				},
				"RevokeConfirmation(address,uint256)": {
					"details": "记录撤销交易日志",
					"params": {
						"owner": "合约所有者反悔撤销投票",
						"txIndex": "交易序号"
					}
				},
				"SubmitTransaction(address,uint256,address,uint256,bytes)": {
					"details": "记录提交的交易日志",
					"params": {
						"data": "交易传递的二进制数据",
						"owner": "交易的提交者",
						"to": "交易的目的地址",
						"txIndex": "交易的序号",
						"value": "交易的金额"
					}
				}
			},
			"kind": "dev",
			"methods": {
				"confirmTransaction(uint256)": {
					"details": "合约所有者中的一个，对其他合约所有者发起的交易投赞成",
					"params": {
						"_txIndex": "交易的索引"
					}
				},
				"executeTransaction(uint256)": {
					"details": "合约所有者中的一个或者交易员",
					"params": {
						"_txIndex": "交易的索引"
					}
				},
				"getOwners()": {
					"details": "返回合约拥有者"
				},
				"getTransaction(uint256)": {
					"details": "返回某个交易信息",
					"params": {
						"_txIndex": "交易索引"
					}
				},
				"getTransactionCount()": {
					"details": "返回交易数量"
				},
				"removeNeedConfirmTransactionIfExected(uint256)": {
					"details": "如果交易完成，删除需要确认的交易",
					"params": {
						"_txIndex": "交易索引"
					}
				},
				"revokeConfirmation(uint256)": {
					"details": "合约所有者中的一个，撤销交易",
					"params": {
						"_txIndex": "交易的索引"
					}
				}
